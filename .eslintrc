{
  "extends": [
    "eslint:recommended",
    "angular"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
    },
  "rules": {
    "quotes": ["error", "double"],
    "keyword-spacing": 2,
    "indent": ["error", 2],
    "angular/controller-as": 0
  },
  "globals": {
    "angular": 1,
    "moment": 1
  }

}
